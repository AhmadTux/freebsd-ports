GREP=/usr/bin/grep
SED=/usr/bin/sed
MODE=ptex-tetex

MKTEXLSR=%%MKTEXLSR%%
TETEX_MODESW=%%TETEX_MODESW%%

case $2 in
POST-INSTALL)
	%%TETEX_MODESW%% ${MODE} dvips forceinstall
	%%MKTEXLSR%%
	;;
DEINSTALL)
	%%TETEX_MODESW%% default dvips forceinstall
	%%MKTEXLSR%%
	;;
esac
