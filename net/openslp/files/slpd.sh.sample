#!/bin/sh
#
# $FreeBSD: ports/net/openslp/files/slpd.sh.sample,v 1.1 2001/09/29 05:42:30 okazaki Exp $
#
# PROVIDE: slpd
# REQUIRE: DAEMON
# KEYWORD: FreeBSD
#
# Add the following line to /etc/rc.conf to enable slpd:
#
# slpd_enable="YES"
#

slpd_enable="NO"
slpd_flags="-p /var/run/slpd.pid"

. %%RC_SUBR%%

name=slpd
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/slpd
pidfile="/var/run/${name}.pid"

load_rc_config ${name}
run_rc_command "$1"
