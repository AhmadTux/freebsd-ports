#!/bin/sh
#
# $FreeBSD: ports/net/vtun/files/vtund.in,v 1.2 2010/03/27 00:14:10 dougb Exp $
#
# PROVIDE: vtund
# REQUIRE: DAEMON
# KEYWORD: shutdown
#
# Add the following line to /etc/rc.conf to enable vtund:
#
# vtund_enable="YES"
#

vtund_enable=${vtund_enable:-"NO"}
vtund_flags=${vtund_flags:-"-s"}

. /etc/rc.subr

name=vtund
rcvar=`set_rcvar`
required_files="%%PREFIX%%/etc/vtund.conf"

command=%%PREFIX%%/sbin/vtund

load_rc_config $name
run_rc_command "$1"
