# PROVIDE: mrouted
# REQUIRE: netif routing cleanvar
# KEYWORD: nojail

. %%RC_SUBR%%

name="mrouted"
rcvar=`set_rcvar`
command=%%PREFIX%%/sbin/${name}
pidfile="/var/run/${name}.pid"
required_files=%%PREFIX%%/etc/${name}.conf
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
