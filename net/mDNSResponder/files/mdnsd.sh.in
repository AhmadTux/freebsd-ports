#!/bin/sh
# $FreeBSD: ports/net/mDNSResponder/files/mdnsd.sh.in,v 1.3 2005/03/26 07:59:14 krion Exp $

# PROVIDE: mdnsd
# REQUIRE: DAEMON
# KEYWORD: FreeBSD shutdown

mdnsd_enable="${mdnsd_enable-NO}"

. %%RC_SUBR%%

name=mdnsd
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/${name}"
pidfile="/var/run/${name}.pid"

load_rc_config $name

run_rc_command $*
