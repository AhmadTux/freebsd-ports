#!/bin/sh
# $FreeBSD: ports/net/mDNSResponder/files/mdnsd.sh.in,v 1.4 2006/01/07 06:29:53 dougb Exp $

# PROVIDE: mdnsd
# REQUIRE: DAEMON
# KEYWORD: shutdown

mdnsd_enable="${mdnsd_enable-NO}"

. %%RC_SUBR%%

name=mdnsd
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/${name}"
pidfile="/var/run/${name}.pid"

load_rc_config $name

run_rc_command $*
