streamingadminserver_enable=${streamingadminserver_enable-"NO"}

. %%RC_SUBR%%

name=streamingadminserver
rcvar=`set_rcvar`
command=%%PREFIX%%/sbin/${name}.pl
command_interpreter=%%PERL%%

load_rc_config ${name}

pidfile=/var/run/streamingadminserver.pid

run_rc_command "$1"
