#!/bin/sh
#
# $FreeBSD: ports/mail/up-imapproxy/files/imapproxyd.sh.in,v 1.1 2005/09/28 16:28:31 mnag Exp $
#

# PROVIDE: imapproxyd
# REQUIRE: LOGIN
# BEFORE: mail
# KEYWORD: shutdown

#
# Add the following line to /etc/rc.conf to enable imapproxyd:
#
#imapproxyd_enable="YES"
#

. %%RC_SUBR%%

name=imapproxyd
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/in.imapproxyd

load_rc_config $name
run_rc_command "$1"
