#!/bin/sh
#
# $FreeBSD: ports/mail/assp/files/assp.sh.in,v 1.3 2006/02/20 20:47:06 dougb Exp $
#

# PROVIDE: assp
# REQUIRE: LOGIN
# KEYWORD: shutdown

. %%RC_SUBR%%

name=assp
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/assp
command_interpreter=%%PERL%%
pidfile=%%ASSP_HOME%%/pid

load_rc_config $name
: ${assp_enable="NO"}
: ${assp_flags="%%ASSP_HOME%%"}

run_rc_command "$1"
