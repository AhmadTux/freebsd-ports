#!/bin/sh
#
# $FreeBSD: ports/mail/assp/files/assp.in,v 1.1 2006/07/18 03:08:23 rafan Exp $
#

# PROVIDE: assp
# REQUIRE: LOGIN
# KEYWORD: shutdown

. %%RC_SUBR%%

name=assp
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/assp
command_interpreter=%%PERL%%
pidfile=%%ASSP_HOME%%/pid

load_rc_config $name
: ${assp_enable="NO"}
: ${assp_args="%%ASSP_HOME%%"}

run_rc_command "$1"
