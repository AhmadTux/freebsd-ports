#!/bin/sh
#
# $FreeBSD: ports/comms/conserver-com/files/conserver.sh.in,v 1.1 2006/01/30 14:02:49 kuriyama Exp $
#

# PROVIDE: conserver
# REQUIRE: DAEMON
# KEYWORD: shutdown

. %%RC_SUBR%%

name="conserver"
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/${name}
pidfile=/var/run/conserver.pid
required_files=%%PREFIX%%/etc/conserver.cf

load_rc_config $name
: ${conserver_enable="NO"}
: ${conserver_flags="-d"}

run_rc_command "$1"
