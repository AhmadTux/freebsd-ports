#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice.org-3/files/openoffice.org-wrapper,v 1.3 2006/04/20 21:56:12 maho Exp $

oopath=%%PREFIX%%/%%INSTALLATION_BASEDIR%%/%%OOODIR%%/program/
program=`echo $0 | sed -e 's|.*-%%OOOVERSION%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
