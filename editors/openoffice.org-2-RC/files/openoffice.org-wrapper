#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice-2.0-devel/files/openoffice.org-wrapper,v 1.1 2005/07/29 01:44:40 maho Exp $

oopath=%%PREFIX%%/%%INSTALLATION_BASEDIR%%/program/
program=`echo $0 | sed -e 's|.*-%%PORTVERSION%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
