#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice.org-3-devel/files/openoffice.org-wrapper,v 1.3 2006/11/12 22:11:35 maho Exp $

oopath=%%PREFIX%%/%%INSTALLATION_BASEDIR%%/%%OOODIR%%/program/
program=`echo $0 | sed -e 's|.*-%%OOOTAG%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
