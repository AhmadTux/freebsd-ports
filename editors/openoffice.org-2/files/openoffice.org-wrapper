#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice.org-2.0/files/openoffice.org-wrapper,v 1.2 2005/08/29 01:06:13 maho Exp $

oopath=%%PREFIX%%/%%INSTALLATION_BASEDIR%%/program/
program=`echo $0 | sed -e 's|.*-%%OOOVERSION%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
