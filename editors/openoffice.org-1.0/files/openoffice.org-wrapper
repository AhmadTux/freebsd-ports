#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice-1.0/files/openoffice-wrapper,v 1.3 2003/01/21 17:09:39 mbr Exp $

oopath=%%PREFIX%%/OpenOffice.org1.0.3/program/
program=`echo $0 | sed -e 's|.*-%%RELEASE_NR%%-||'`

case $program in
$0)
	exec $oopath/soffice "$@"
	;;
	*)
	exec $oopath/$program "$@"
	;;
esac
