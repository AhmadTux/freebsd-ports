#!/bin/sh
#
# $FreeBSD: ports/editors/openoffice-1.1/files/openoffice-wrapper,v 1.15 2005/01/11 23:17:42 maho Exp $

oopath=%%PREFIX%%/%%INSTALLATION_BASEDIR%%/program/
program=`echo $0 | sed -e 's|.*-%%RELEASE_NR%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
