#!/bin/sh

# $FreeBSD: ports/textproc/iiimf-server/files/iiimd.sh.in,v 1.1 2005/05/24 12:37:49 daichi Exp $

# PROVIDE: iiimd
# REQUIRE: DAEMON
# KEYWORD: shutdown

prefix=%%PREFIX%%

iiimd_enable=${iiimd_enable:-"NO"}

. %%RC_SUBR%%

name="iiimd"
rcvar=`set_rcvar`
command="${prefix}/bin/iiimd"

load_rc_config $name
command_args="> /dev/null 2>&1 &"
run_rc_command "$1"
