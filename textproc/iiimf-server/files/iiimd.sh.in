#!/bin/sh

# $FreeBSD: ports/textproc/iiimf-server/files/iiimd.sh.in,v 1.3 2009/07/15 16:56:07 dougb Exp $

# PROVIDE: iiimd
# REQUIRE: DAEMON
# KEYWORD: shutdown

iiimd_enable=${iiimd_enable:-"NO"}

. /etc/rc.subr

name="iiimd"
rcvar=`set_rcvar`
command="%%PREFIX%%/bin/iiimd"

load_rc_config $name
command_args="> /dev/null 2>&1 &"
run_rc_command "$1"
