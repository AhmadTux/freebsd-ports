#!/bin/sh
# $FreeBSD: ports/x11/gdm/files/gdm.sh.in,v 1.4 2005/06/10 02:13:44 jylefort Exp $

# PROVIDE: gdm
# REQUIRE: DAEMON
# KEYWORD: FreeBSD shutdown

gdm_enable=${gdm_enable-"NO"}

. %%RC_SUBR%%

name=gdm
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/${name}"
pidfile="/var/run/${name}.pid"
procname="%%PREFIX%%/sbin/gdm-binary"

load_rc_config ${name}
run_rc_command "$1"
