#!/bin/sh

# $FreeBSD: ports/sysutils/usermin/files/usermin.sh.sample,v 1.1 2002/11/10 20:11:29 olgeni Exp $

case $1 in

	start)
		if [ -x !!PREFIX!!/etc/usermin/start ]; then
			!!PREFIX!!/etc/usermin/start >/dev/null
			echo -n ' usermin'
		fi
		exit 0
		;;

	stop)
		if [ -x !!PREFIX!!/etc/usermin/stop ]; then
			!!PREFIX!!/etc/usermin/stop >/dev/null
			echo -n ' usermin'
		fi
		exit 0
		;;

	*)
		echo "usage: `basename $0` {start|stop}" >&2
		exit 64
		;;
esac
