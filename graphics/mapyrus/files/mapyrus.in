if [ -z "${DISPLAY}" ]; then
    JAVA_AWT_HEADLESS="-Djava.awt.headless=true"
else
    JAVA_AWT_HEADLESS=""
fi
JAVA_VERSION="%%JAVA_VERSION%%" \
	exec "%%LOCALBASE%%/bin/java" "${JAVA_AWT_HEADLESS}" -jar "%%DATADIR%%/%%MAPYRUSJAR%%" "$@"
