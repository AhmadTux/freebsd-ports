#!/bin/sh
#
# $FreeBSD: ports/databases/tokyotyrant/files/ttserver.in,v 1.1 2009/03/27 21:34:43 lwhsu Exp $
#
#
# PROVIDE: ttserver
# REQUIRE: DAEMON
# KEYWORD: shutdown

. %%RC_SUBR%%

name="ttserver"
rcvar=`set_rcvar`

: ${ttserver_enable:="NO"}

command="%%PREFIX%%/bin/${name}"
pidfile="/var/run/ttserver.pid"

load_rc_config $name
: ${ttserver_flags="-dmn -pid ${pidfile}"}

run_rc_command "$1"
