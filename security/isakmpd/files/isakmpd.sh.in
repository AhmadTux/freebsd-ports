# PROVIDE: ike isakmpd
# REQUIRE: root mountcritlocal
# BEFORE:  DAEMON
# KEYWORD: nojail

. %%RC_SUBR%%

name="isakmpd"
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/${name}"
extra_commands="reload"

load_rc_config $name

: ${isakmpd_enable="NO"}

run_rc_command "$1"
