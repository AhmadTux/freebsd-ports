#!/bin/sh
#
# $FreeBSD: ports/security/xinetd/files/xinetd.sh.tmpl,v 1.1 2004/02/23 14:47:59 vanilla Exp $
#
# PROVIDE: snmpd
# REQUIRE: DAEMON
#
# Add the following line to /etc/rc.conf to enable xinetd:
#
# xinetd_enable="YES"
#

. %%RC_SUBR%%

name=xinetd
rcvar=`set_rcvar`

command=%%PREFIX%%/sbin/xinetd > /dev/null 2>&1

xinetd_enable=${xinetd_enable:-"NO"}
xinetd_flags=${xinetd_flags:-"-f %%PREFIX%%/etc/xinetd.conf"}

load_rc_config $name
run_rc_command "$1"
