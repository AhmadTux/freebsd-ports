#!/bin/sh
#
# $FreeBSD: ports/devel/cvsnt/files/cvslockd.sh.in,v 1.1 2005/04/19 19:03:52 fjoe Exp $
#
# PROVIDE: cvslockd
# REQUIRE: LOGIN
# KEYWORD: shutdown
#
. %%RC_SUBR%%

[ -z "$cvslockd_enable" ] && cvslockd_enable="NO"

name=cvslockd
rcvar=`set_rcvar`
command=%%PREFIX%%/bin/${name}

load_rc_config ${name}
run_rc_command "$1"
