#!/bin/sh
#
# $FreeBSD: ports/devel/cvsnt/files/cvslockd.sh,v 1.1 2004/07/31 18:18:42 fjoe Exp $
#
# PROVIDE: cvslockd
# REQUIRE: LOGIN
# KEYWORD: FreeBSD shutdown
#
. %%RC_SUBR%%

[ -z "$cvslockd_enable" ] && cvslockd_enable="NO"

name=cvslockd
rcvar=`set_rcvar`
command=%%PREFIX%%/bin/${name}

load_rc_config ${name}
run_rc_command "$1"
