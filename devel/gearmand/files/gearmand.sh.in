#!/bin/sh
#
# $FreeBSD: ports/devel/gearmand/files/gearmand.sh.in,v 1.2 2009/01/30 05:18:43 rafan Exp $
#

# PROVIDE: gearmand
# REQUIRE: DAEMON

. "%%RC_SUBR%%"

name="gearmand"
command="%%PREFIX%%/bin/gearmand"

load_rc_config "$name"
: ${gearmand_enable="NO"}
: ${gearmand_flags="-d -v"}
: ${gearmand_group="daemon"}
: ${gearmand_user="daemon"}

run_rc_command "$1"

