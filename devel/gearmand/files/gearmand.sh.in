#!/bin/sh
#
# $FreeBSD: ports/devel/gearmand/files/gearmand.sh.in,v 1.4 2009/07/01 08:58:20 miwi Exp $
#

# PROVIDE: gearmand
# REQUIRE: DAEMON

. "%%RC_SUBR%%"

name="gearmand"
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/gearmand"

load_rc_config "$name"
: ${gearmand_enable="NO"}
: ${gearmand_flags="-d -v"}
: ${gearmand_group="daemon"}
: ${gearmand_user="daemon"}

run_rc_command "$1"

