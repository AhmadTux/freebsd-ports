#

# PROVIDE: gearmand
# REQUIRE: DAEMON

. "%%RC_SUBR%%"

name="gearmand"
command="%%PREFIX%%/sbin/gearmand"

load_rc_config "$name"
: ${gearmand_enable="NO"}
: ${gearmand_flags="-d -v"}
: ${gearmand_group="daemon"}
: ${gearmand_user="daemon"}

run_rc_command "$1"

